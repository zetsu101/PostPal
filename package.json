{
  "name": "postpal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:ws": "node -r ts-node/register src/lib/websocket-server-startup.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:ws\"",
    "build": "next build",
    "start": "next start",
    "start:ws": "node dist/lib/websocket-server-startup.js",
    "start:full": "concurrently \"npm run start\" \"npm run start:ws\"",
    "lint": "next lint",
    "test": "node -r ts-node/register src/lib/__tests__/test-runner.ts",
    "test:websocket": "node -r ts-node/register src/lib/__tests__/websocket-test-suite.ts",
    "test:integration": "node -r ts-node/register src/lib/__tests__/ai-insights-integration-test.ts",
    "test:api": "ts-node --project src/lib/__tests__/tsconfig.json src/lib/__tests__/run-api-tests.ts",
    "test:watch": "nodemon --exec \"npm run test\" --watch src/lib --ext ts,tsx",
    "test:ci": "npm run test && npm run lint",
    "db:verify": "node -r ts-node/register scripts/verify-database.ts",
    "check:production": "bash scripts/check-production-ready.sh",
    "deploy:production": "bash scripts/deploy-production.sh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.908.0",
    "@aws-sdk/s3-request-presigner": "^3.908.0",
    "@react-email/components": "^0.5.6",
    "@sentry/nextjs": "^10.19.0",
    "@sentry/profiling-node": "^10.19.0",
    "@stripe/react-stripe-js": "^3.8.1",
    "@stripe/stripe-js": "^7.9.0",
    "@supabase/supabase-js": "^2.58.0",
    "compression": "^1.8.1",
    "concurrently": "^9.1.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "firebase": "^12.0.0",
    "framer-motion": "^12.23.11",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "lucide-react": "^0.539.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "next": "15.4.8",
    "next-swagger-doc": "^0.4.1",
    "openai": "^5.10.2",
    "react": "19.1.2",
    "react-dom": "19.1.2",
    "react-dropzone": "^14.3.8",
    "react-email": "^4.3.0",
    "recharts": "^3.1.2",
    "resend": "^6.1.2",
    "sharp": "^0.34.4",
    "stripe": "^19.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.18.3",
    "ws": "^8.18.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.5.12",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.4.8",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "dotenv": "^16.4.5"
  }
}
